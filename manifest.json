{
  "name": "SegmentFault Notifier",
  "description": "SegmentFault消息提醒",
  "icons": {
    "128": "logo-128.png",
    "16": "logo-16.png",
    "48": "logo-48.png"
  },
  "browser_action": {
    "default_icon": "logo-48.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "manifest_version": 2,
  "permissions": [
    "https://segmentfault.com/*"
  ],
  "version": "1.1.0"
}